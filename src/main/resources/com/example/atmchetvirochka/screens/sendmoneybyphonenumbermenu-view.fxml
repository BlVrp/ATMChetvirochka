<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.atmchetvirochka.controller.SendMoneyByPhoneNumber">
   <AnchorPane prefHeight="204.0" prefWidth="600.0">
      <Button layoutX="19.0" layoutY="145.0" mnemonicParsing="false" onAction="#backToMainMenu" prefHeight="50.0" prefWidth="50.0" text="BACK" textFill="#000000c3">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
      <Button layoutX="499.0" layoutY="83.0" mnemonicParsing="false" onAction="#changeFieldDown" prefHeight="50.0" prefWidth="50.0" text="Down" textFill="#000000a6">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button layoutX="499.0" layoutY="14.0" mnemonicParsing="false" onAction="#changeFieldUp" prefHeight="50.0" prefWidth="50.0" text="Up" textFill="#000000a4">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button layoutX="499.0" layoutY="145.0" mnemonicParsing="false" onAction="#sendMoney" prefHeight="50.0" prefWidth="50.0" text="Enter" textFill="#000000c3">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
      <Label layoutX="56.0" prefHeight="50.0" prefWidth="383.0" text="Enter phone number and amount of money" textFill="#000000c1">
         <font>
            <Font name="System Italic" size="20.0" />
         </font>
      </Label>
      <Label fx:id="phoneNumber" layoutX="164.0" layoutY="77.0" prefHeight="50.0" prefWidth="221.0" style="-fx-background-color: #00000010;" text="PHONE" textFill="#00000067">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label fx:id="cashNumber" layoutX="164.0" layoutY="133.0" prefHeight="50.0" prefWidth="221.0" style="-fx-background-color: #00000010;" text="MONEY" textFill="#00000066">
         <font>
            <Font name="System Bold" size="23.0" />
         </font>
      </Label>
      <Label fx:id="exception" layoutX="69.0" layoutY="39.0" prefHeight="31.0" prefWidth="323.0">
         <font>
            <Font size="14.0" />
         </font></Label>
   </AnchorPane>
   <fx:include fx:id="editButtons" layoutY="210" source="editbutton-view.fxml" />
</AnchorPane>
