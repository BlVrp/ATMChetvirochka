<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.atmchetvirochka.controller.SendMoneyForCharityMenuController">
   <AnchorPane prefHeight="212.0" prefWidth="600.0">
      <Label layoutX="16.0" layoutY="-8.0" prefHeight="88.0" prefWidth="568.0" text="Here you can`t make choice about place where your money will &#10;send - only cash number" textFill="#000000a4">
         <font>
            <Font name="System Italic" size="20.0" />
         </font>
      </Label>
      <Button layoutX="14.0" layoutY="149.0" mnemonicParsing="false" onAction="#backToMainMenu" prefHeight="52.0" prefWidth="52.0" text="BACK" textFill="#000000c1">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
      <Button layoutX="532.0" layoutY="149.0" mnemonicParsing="false" onAction="#sendMoney" prefHeight="52.0" prefWidth="52.0" text="Enter" textFill="#000000c2">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
      <Label fx:id="cashNumber" layoutX="183.0" layoutY="106.0" prefHeight="52.0" prefWidth="234.0" style="-fx-background-color: #00000010;" text="MONEY" textFill="#00000065">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label fx:id="exception" layoutX="16.0" layoutY="67.0" prefHeight="26.0" prefWidth="446.0">
         <font>
            <Font size="14.0" />
         </font></Label>
   </AnchorPane>
   <fx:include fx:id="editButtons" layoutY="210" source="editbutton-view.fxml" />
</AnchorPane>
